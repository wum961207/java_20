<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testNG测试项目">
    <test name="注册接口">
        <classes>
            <class name="com.lemon.easyApiTest.cases.RegisterCase"></class>
            <parameter name="sheetIndex" value="0"></parameter>
            <parameter name="sheetNum" value="1"></parameter>
        </classes>
    </test>


    <test name="登录接口">
        <classes>
            <class name="com.lemon.easyApiTest.cases.LoginCase"></class>
            <parameter name="sheetIndex" value="1"></parameter>
            <parameter name="sheetNum" value="1"></parameter>
        </classes>
    </test>

    <test name="充值接口">
        <classes>
            <class name="com.lemon.easyApiTest.cases.RechargeCase"></class>
            <parameter name="sheetIndex" value="2"></parameter>
            <parameter name="sheetNum" value="1"></parameter>
        </classes>
    </test>
</suite>